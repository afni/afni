### Make the source distribution

afni_src.tgz:
	$(MKDIR) afni_src
	$(CP) *.c *.f *.inc *.h *.hhh Makefile.* *.xbm $(MINDOCS) $(MISC) $(SCRIPTS) ./afni_src
	$(CP) -r coxplot volpack moveup moveAFNI* cdflib eispack ./afni_src
	$(CP) -r gifsicledir qhulldir f2cdir mpeg_encodedir dlcompat ./afni_src
	$(CP) -r netcdf-3.5.0 jpeg-6b niml ./afni_src
	$(CP) -r SUMA rickr faces ./afni_src
	$(TAR) cf - afni_src | $(GZIP) -9v > afni_src.tgz
	$(RM) -r afni_src

### Make the documentation distribution: only for use by RWCox

afni_doc.tgz:
	$(RM) $@
	$(MKDIR) ./afni_doc
	$(CP) $(DOCS) ./afni_doc
	$(TAR) cf - ./afni_doc | $(GZIP) -9v > afni_doc.tgz
	$(RM) -r ./afni_doc

### Put things in their places

install: $(INSTALLDIR)
	$(MV) $(PROGRAMS) $(INSTALLDIR)
	$(CP) $(SCRIPTS) $(INSTALLDIR)

install_noplug: $(INSTALLDIR)
	$(MV) $(PROGRAMS_NOPLUG) $(INSTALLDIR)
	$(CP) $(SCRIPTS) $(INSTALLDIR)

install_lib: $(LIBDIR)
	$(CP) libmri.a libf2c.a libcoxplot.a libvolpack.a $(LIBHEADERS) $(LIBDIR)

# CYGWIN: must put the .exe suffix on the "mv" command for some reason

install_exe: $(INSTALLDIR)
	( for pp in $(PROGRAMS_EXE) ; do                               \
             qq=`basename $$pp .exe`; $(MV) $${qq}.exe $(INSTALLDIR) ; \
          done )
	$(CP) $(SCRIPTS) $(INSTALLDIR)


	$(RM) *.a $(PROGRAMS)

cleanest:clean_version
	$(RM) `find . -name \*.o -print`
	$(RM) `find . -name \*.so -print`
	$(RM) `find . -name \*.a -print`
	$(RM) $(PROGRAMS)
	$(RM) SUMA/SUMA_*.tar


# Create program list
echo_progs:
	echo "$(PROGRAMS) `\ls $(SCRIPTS_DIR)` $(SUMA_PROGS) $(SUMA_GTS_PROGS)   \
             $(PY_SCRIPTS)" | tr ' ' '\n' | LC_COLLATE=C sort | uniq

prog_list:
	echo "#File content generated by 'make prog_list'" > prog_list.txt
	echo "#You can compare its contents to the inferred list from:" \
                                                     >> prog_list.txt
	echo "#  apsearch -list_all_afni_progs | LC_COLLATE=C sort | uniq" \
		>> prog_list.txt
	echo "" >> prog_list.txt
	echo "$(PROGRAMS) `\ls $(SCRIPTS_DIR)` $(SUMA_PROGS) $(SUMA_GTS_PROGS)    \
                $(PY_SCRIPTS)" | tr ' ' '\n' | LC_COLLATE=C sort | uniq \
		> ___prog_list.txt
	make -s -f ptaylor/Makefile.ptaylor.INCLUDE echo_progs | \
		tr ' ' '\n' | LC_COLLATE=C sort | uniq >> ___prog_list.txt
	cat ___prog_list.txt | LC_COLLATE=C sort | uniq >> prog_list.txt
	\rm -f ___prog_list.txt

# Create .gitignore under src/
gitignore: prog_list
	echo '##              DO NOT EDIT   ' > .gitignore
	echo '##This file is created with "make gitignore". ' >> .gitignore
	echo '##Changes should be made to sources used to create it:' >> .gitignore
	echo '##                            AFNI/.gitignore '>> .gitignore
	echo '##                            AFNI/src/gitignore.src.txt '>> .gitignore
	echo '##and/or the variables listing all programs in src/Makefile.INCLUDE ' \
                                                                  >> .gitignore
	echo '##targets echo_progs, and prog_list.'  >> .gitignore
	echo '##Run "make gitignore" to recreate src/.gitignore ' >> .gitignore
	if [ -f ../.gitignore ]; then \
      echo "" >> .gitignore; \
      echo "##Contents of master AFNI/.gitignore" >> .gitignore; \
      cat ../.gitignore >> .gitignore; \
   fi
	echo "" >> .gitignore
	echo "##Contents of gitignore.src.txt" >> .gitignore
	cat gitignore.src.txt >> .gitignore
	echo "" >> .gitignore
	echo "##Ignore all the programs and scripts" >> .gitignore
	echo "##List obtained from AFNI/src/prog_list.txt" >> .gitignore
	cat prog_list.txt >> .gitignore


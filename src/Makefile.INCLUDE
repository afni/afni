### Make everything, install it too, and cleanup

totality:all plugins install install_plugins install_lib clean

### Make everything, install it too, but don't cleanup

vastness:all plugins install install_plugins install_lib

### Make many things

all:$(LIBDL) libmri.a libcoxplot.a libvolpack.a programs

### Make a few more things

itall:all plugins install install_plugins

$(INSTALLDIR):
	if [ ! -d $(INSTALLDIR) ]; then $(MKDIR) -p $(INSTALLDIR); echo Made directory $(INSTALLDIR); fi

### for those on crippled systems

all_noplug:libmri.a programs_noplug

totality_noplug:libmri.a libcoxplot.a programs_noplug install_noplug clean

# CYGWIN: variations on totality and vastness above

totality_exe:libmri.a libcoxplot.a programs_exe install_exe clean_exe
vastness_exe:libmri.a libcoxplot.a programs_exe install_exe

programs:$(PROGRAMS)

programs_noplug:$(PROGRAMS_NOPLUG)

programs_exe:$(PROGRAMS_EXE)

libraries:libmri.a libcoxplot.a libvolpack.a libf2c.a


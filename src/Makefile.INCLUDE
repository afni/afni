R_io.so:R_io.c libmri.a
	( if which R >/dev/null ; then \
	echo PKG_CPPFLAGS = $(INFLAGS)  > Makevars ;\
	echo PKG_LIBS =  $(LFLAGS) $(LLIBS) $(RLIB_CONVERT) >> Makevars ;\
	R CMD SHLIB -o R_io.so R_io.c ; \
	else echo WARNING: Cannot build R_io.so because R is not installed;\
	fi ; )

####
# version system:
#    AFNI_version_base.txt: updated by build scripts (dist_src)
#    libmri.h depends on AFNI_version.h, which depends on AFNI_version_base.txt
#
#    AFNI_version.txt: depends on AFNI_version_base.txt and programs
#             - this is just an install file
#    all:     depends on AFNI_version.txt
#    install: installs AFNI_version.txt

mrilib.h: Startup_Commands AFNI_version.h

# the purpose of this is to run any commands needed before
# actually starting compilation:
#   in particular, for the NIH Biowulf, it is necessary to
#   do 'module load intel' to gain access to the Intel C compiler

Startup_Commands:
	$(STARTUP_COMMANDS)

AFNI_version.h: AFNI_version_base.txt
	echo "#undef  AFNI_VERSION_LABEL" > AFNI_version.h ;                 \
	echo '#define AFNI_VERSION_LABEL    "'`cat AFNI_version_base.txt`'"' \
             >> AFNI_version.h ;                                             \
	echo "#undef  AFNI_VERSION_PLATFORM" >> AFNI_version.h ;             \
	echo '#define AFNI_VERSION_PLATFORM "'$(SYSTEM_NAME)'"'              \
             >> AFNI_version.h ;

# no .txt in target, so that it always updates the file when invoked
AFNI_version:
	( $(CP) AFNI_version_base.txt AFNI_version.txt ; \
	echo $(SYSTEM_NAME) >> AFNI_version.txt ;        \
	date +'%b %d %Y' >> AFNI_version.txt ;           \
	)


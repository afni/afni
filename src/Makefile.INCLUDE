	tar -xf $(SUMA_BIN_ARCHIVE)	;\
	touch *.a	;\
	$(CP) libmri.a libtractools.a libmrix.a libcoxplot.a libf2c.a ../ ;\
	$(RM) suma MakeColorMap ROI2dataset 3dVol2Surf 3dSurf2Vol 	\
   SurfMeasures  SurfFWHM ScaleToMap MapIcosahedron CreateIcosahedron 	\
   CompareSurfaces ConvertSurface ConvertDset SurfClust IsoSurface 	\
   ConvexHull SurfaceMetrics SurfSmooth inspec 				\
   quickspec FSread_annot SurfPatch SurfQual SampBias 3dSkullStrip 	\
   3dCRUISEtoAFNI 3dBRAIN_VOYAGERtoAFNI 3dSurfMask SurfToSurf		\
   Surf2VolCoord ROIgrow SurfDsetInfo SurfInfo AnalyzeTrace DriveSuma 	\
   SurfDist SpharmReco SpharmDeco prompt_user prompt_popup SurfRetinoMap\
   ParseName 3dGenPriors 3dGenFeatureDist 3dSeg SurfExtrema 3dinfill 	\
   3dHist HalloSuma niprobe BrainSkin InstaTract; 			\
	$(MAKE) -f $(SUMA_MAKEFILE_NAME) testGL suma MakeColorMap	\
   ROI2dataset 3dVol2Surf 3dSurf2Vol ScaleToMap SurfMeasures SurfFWHM 	\
   MapIcosahedron CreateIcosahedron CompareSurfaces ConvertSurface 	\
   ConvertDset SurfClust IsoSurface ConvexHull SurfaceMetrics SurfSmooth\
   inspec quickspec FSread_annot SurfPatch SurfQual SampBias 		\
   3dSkullStrip 3dCRUISEtoAFNI 3dBRAIN_VOYAGERtoAFNI 3dSurfMask 	\
   SurfToSurf Surf2VolCoord ROIgrow SurfDsetInfo SurfInfo AnalyzeTrace  \
   DriveSuma SurfDist SpharmReco SpharmDeco prompt_user prompt_popup	\
   SurfRetinoMap ParseName 3dGenPriors 3dGenFeatureDist 3dSeg 		\
   SurfExtrema 3dinfill 3dHist HalloSuma InstaTract niprobe BrainSkin 	\
	'INSTALLDIR=$(INSTALLDIR)' \
	'SUMA_MDEFS=$(SUMA_MDEFS)' \
	'SUMA_INCLUDE_PATH=$(SUMA_INCLUDE_PATH)' \
